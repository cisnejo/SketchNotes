{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "My extension description",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["inject.js"],
      "css": ["inject.css"]
    }
  ],
  "web_accessible_resources": [
    "overlay.html"
  ],
  "permissions": [
    "storage",
    "tabs"
]
}
